{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "SQL Server",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName",
          "widget-attributes": {
            "placeholder": "Name used to identify this sink for lineage"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Driver Name",
          "name": "jdbcPluginName",
          "widget-attributes": {
            "default": "sqlserver42"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host",
          "widget-attributes": {
            "default": "localhost"
          }
        },
        {
          "widget-type": "number",
          "label": "Port",
          "name": "port",
          "widget-attributes": {
            "default": "1433"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Database",
          "name": "database"
        },
        {
          "widget-type": "textbox",
          "label": "Table Name",
          "name": "tableName"
        },
        {
          "widget-type": "hidden",
          "label": "Schema Name",
          "name": "dbSchemaName"
        }
      ]
    },
    {
      "label": "Credentials",
      "properties": [
        {
          "label": "Authentication Type",
          "name": "authentication",
          "widget-type": "radio-group",
          "widget-attributes": {
            "layout": "inline",
            "default": "SQL Login",
            "options": [
              {
                "id": "SQL Login",
                "label": "SQL Login"
              },
              {
                "id": "ActiveDirectoryPassword",
                "label": "Active Directory Password"
              }
            ]
          }
        },
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "user"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        }
      ]
    },
    {
      "label": "Advanced",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Instance Name",
          "name": "instanceName"
        },
        {
          "widget-type": "number",
          "label": "Query Timeout",
          "name": "queryTimeout",
          "widget-attributes": {
            "default": "-1"
          }
        },
        {
          "widget-type": "number",
          "label": "Connect Timeout",
          "name": "loginTimeout",
          "widget-attributes": {
            "default": "0"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Column Encryption",
          "name": "columnEncryptionSetting",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "Enabled"
            },
            "off": {
              "value": "false",
              "label": "Disabled"
            },
            "default": "false"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Encrypt",
          "name": "encrypt",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "Yes"
            },
            "off": {
              "value": "false",
              "label": "No"
            },
            "default": "false"
          }
        },
        {
          "widget-type": "toggle",
          "label": "Trust Server Certificate",
          "name": "trustServerCertificate",
          "widget-attributes": {
            "on": {
              "value": "true",
              "label": "Yes"
            },
            "off": {
              "value": "false",
              "label": "No"
            },
            "default": "false"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Workstation ID",
          "name": "workstationID"
        },
        {
          "widget-type": "textbox",
          "label": "Failover Partner",
          "name": "failoverPartner"
        },
        {
          "widget-type": "number",
          "label": "Packet Size",
          "name": "packetSize",
          "widget-attributes": {
            "default": "-1"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Current Language",
          "name": "currentLanguage"
        },
        {
          "widget-type": "keyvalue",
          "label": "Connection Arguments",
          "name": "connectionArguments",
          "widget-attributes": {
            "showDelimiter": "false",
            "key-placeholder": "Key",
            "value-placeholder": "Value",
            "kv-delimiter": "=",
            "delimiter": ";"
          }
        }
      ]
    }
  ],
  "outputs": [],
  "jump-config": {
    "datasets": [
      {
        "ref-property-name": "referenceName"
      }
    ]
  }
}
