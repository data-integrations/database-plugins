{
  "metadata": {
    "spec-version": "1.5"
  },
  "display-name": "Oracle",
  "configuration-groups": [
    {
      "label": "Basic",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName",
          "widget-attributes": {
            "placeholder": "Name used to identify this sink for lineage"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Driver Name",
          "name": "jdbcPluginName",
          "widget-attributes": {
            "default": "oracle"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Host",
          "name": "host",
          "widget-attributes": {
            "default": "localhost"
          }
        },
        {
          "widget-type": "number",
          "label": "Port",
          "name": "port",
          "widget-attributes": {
            "default": "1521"
          }
        },
        {
          "name": "connectionType",
          "label": "Connection Type",
          "widget-type": "radio-group",
          "widget-attributes": {
            "layout": "inline",
            "default": "SID",
            "options": [
              {
                "id": "SID",
                "label": "SID"
              },
              {
                "id": "service",
                "label": "Service Name"
              },
              {
                "id": "TNS",
                "label": "TNS Connect Descriptor"
              }
            ]
          }
        },
        {
          "widget-type": "textbox",
          "label": "SID/Service Name/TNS Connect Descriptor",
          "description": "Oracle connection point (Database name, Service name, or TNS Connect Descriptor)",
          "name": "database"
        },
        {
          "widget-type": "textbox",
          "label": "Table Name",
          "name": "tableName"
        }
      ]
    },
    {
      "label": "Credentials",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "user"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        }
      ]
    },
    {
      "label": "Advanced",
      "properties": [
        {
          "widget-type": "keyvalue",
          "label": "Connection Arguments",
          "name": "connectionArguments",
          "widget-attributes": {
            "showDelimiter": "false",
            "key-placeholder": "Key",
            "value-placeholder": "Value",
            "kv-delimiter": "=",
            "delimiter": ";"
          }
        },
        {
          "widget-type": "number",
          "label": "Default Batch Value",
          "name": "defaultBatchValue",
          "widget-attributes": {
            "default": "10",
            "min": "1"
          }
        }
      ]
    }
  ],
  "outputs": [],
  "jump-config": {
    "datasets": [
      {
        "ref-property-name": "referenceName"
      }
    ]
  }
}
